{
    "name": "paulo/repository-pattern-generator",
    "description": "Generator for repositories.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Paulo Santos",
        "email": "paulo.santos@ideavity.com"
    }],
    "version": "1.0",
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Paulo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paulo\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Paulo\\RepositoryServiceProvider"
            ],
            "dont-discover": []
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^8.3"
    }
}
